{
    "name": "@personal/time-tracker",
    "version": "0.1.0",
    "description": "Time Tracker Logseq Plugin with React",
    "main": "dist/index.html",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "test:coverage": "vitest --coverage",
        "type-check": "tsc --noEmit",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md,css.ts}\"",
        "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md,css.ts}\""
    },
    "keywords": [
        "logseq-plugin",
        "react",
        "typescript"
    ],
    "license": "MIT",
    "dependencies": {
        "@logseq/libs": "^0.0.17",
        "@vanilla-extract/css": "catalog:",
        "react": "catalog:",
        "react-dom": "catalog:"
    },
    "devDependencies": {
        "@eslint/js": "catalog:",
        "@testing-library/jest-dom": "catalog:",
        "@testing-library/react": "catalog:",
        "@types/node": "catalog:",
        "@types/react": "catalog:",
        "@types/react-dom": "catalog:",
        "@vanilla-extract/vite-plugin": "catalog:",
        "@vitejs/plugin-react": "catalog:",
        "@vitest/coverage-v8": "catalog:",
        "@vitest/ui": "catalog:",
        "eslint": "catalog:",
        "eslint-config-prettier": "catalog:",
        "eslint-plugin-prettier": "catalog:",
        "eslint-plugin-react-hooks": "catalog:",
        "eslint-plugin-react-refresh": "catalog:",
        "globals": "catalog:",
        "jsdom": "catalog:",
        "prettier": "catalog:",
        "typescript": "catalog:",
        "typescript-eslint": "catalog:",
        "vite": "catalog:",
        "vite-plugin-logseq": "^1.1.2",
        "vitest": "catalog:"
    },
    "logseq": {
        "id": "logseq-time-tracker",
        "title": "Time Tracker",
        "icon": "./logo.svg",
        "main": "dist/index.html"
    }
}
