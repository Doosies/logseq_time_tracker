{
    "name": "@personal/plugin",
    "version": "0.1.0",
    "description": "Personal Logseq Plugin with React",
    "main": "dist/index.html",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "test:coverage": "vitest --coverage",
        "type-check": "tsc --noEmit",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md,css.ts}\"",
        "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md,css.ts}\""
    },
    "keywords": [
        "logseq-plugin",
        "react",
        "typescript"
    ],
    "license": "MIT",
    "dependencies": {
        "@logseq/libs": "^0.0.17",
        "@vanilla-extract/css": "^1.18.0",
        "react": "^19.2.4",
        "react-dom": "^19.2.4"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.2",
        "@testing-library/jest-dom": "^6.9.1",
        "@testing-library/react": "^16.3.2",
        "@types/node": "^25.0.10",
        "@types/react": "^19.2.10",
        "@types/react-dom": "^19.2.3",
        "@vanilla-extract/vite-plugin": "^5.1.4",
        "@vitejs/plugin-react": "^5.1.2",
        "@vitest/coverage-v8": "^4.0.18",
        "@vitest/ui": "^4.0.18",
        "eslint": "^9.39.2",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-prettier": "^5.5.5",
        "eslint-plugin-react-hooks": "^7.0.1",
        "eslint-plugin-react-refresh": "^0.4.26",
        "globals": "^17.2.0",
        "jsdom": "^27.4.0",
        "prettier": "^3.8.1",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.54.0",
        "vite": "^7.3.1",
        "vite-plugin-logseq": "^1.1.2",
        "vitest": "^4.0.18"
    },
    "logseq": {
        "id": "logseq-plugin-personal",
        "title": "Personal Plugin",
        "icon": "./logo.svg",
        "main": "dist/index.html"
    }
}
